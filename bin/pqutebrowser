#!/usr/bin/env sh

# run in temporary configuration setting disabling canvas reading and enabling
# private browsing right away for better privacy.
qutebrowser \
            -TC /home/simon/.config/qutebrowser/config.py \
            -s content.private_browsing true \
            -s content.canvas_reading false

